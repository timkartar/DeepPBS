! MolScript input file generated by 3DNA

plot

    read mol "t.pdb";
    set colourparts off;
    set segments 6, plane2colour grey 0.8;

! protein part

! Nucleic acid colored by chain IDs

    set coilradius 0.80;
    set planecolour red;
    double-helix chain A;
    set planecolour yellow;
    double-helix chain B;
    set planecolour lawngreen;
    double-helix chain C;
    set planecolour cyan;
    double-helix chain D;

end_plot
